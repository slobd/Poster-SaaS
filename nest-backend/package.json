    "scripts": {
        "precommit": "lint-staged -v",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"*.json\"",
        "format:check": "prettier \"src/**/*.ts\" \"test/**/*.ts\" --check",
        "start": "nest start",
        "dev": "prisma db push && yarn start:dev",
        "start:dev": "cross-env NODE_ENV=development nest start --watch --path tsconfig.json",
        "start:debug": "prisma db push && cross-env NODE_ENV=development nest start --debug --watch --path tsconfig.json",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env NODE_ENV=test && jest --config ./jest-e2e.json --runInBand --testTimeout=30000",
        "docs": "compodoc -p tsconfig.json -s",
        "prisma:generate": "prisma generate"
    },
